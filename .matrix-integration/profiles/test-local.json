{
  "name": "test-local",
  "version": "1.0.1",
  "created": "2026-01-26T00:00:00.000Z",
  "author": "user",
  "description": "Local development and testing profile with debug features",
  "env": {
    "API_ENDPOINT": "http://localhost:3000",
    "DATABASE_URL": "${DATABASE_URL}",
    "LOG_LEVEL": "debug",
    "FEATURE_FLAGS": "caching,debug-mode,test-features",
    "LOCAL_ONLY": "true",
    "NODE_ENV": "development",
    "DEBUG": "app:*",
    "ENABLE_MOCK_DATA": "true",
    "DISABLE_AUTH": "false",
    "TEST_COVERAGE": "true",
    "RESET_DATABASE": "false",
    "SLOW_QUERY_LOG": "true",
    "API_TIMEOUT": "60000",
    "CORS_ORIGINS": "http://localhost:3000,http://localhost:5173",
    "DEV_TOOLS": "true"
  }
}
