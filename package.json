{
  "name": "matrix-analysis",
  "version": "3.2.0",
  "engines": {
    "bun": ">=1.0.0"
  },
  "author": "nolarose",
  "types": "types.ts",
  "exports": {
    ".": "./50-col-matrix.ts",
    "./types": "./types.ts"
  },
  "devDependencies": {
    "typescript": "^5.9.3"
  },
  "peerDependencies": {
    "@types/bun": "^1.3.6"
  },
  "description": "URLPattern Analysis Matrix with 219 columns - Performance optimized",
  "license": "MIT",
  "peerDependenciesMeta": {
    "@types/bun": {
      "optional": true
    }
  },
  "scripts": {
    "matrix": "bun 50-col-matrix.ts",
    "matrix:audit": "bun 50-col-matrix.ts --audit",
    "matrix:benchmark": "bun 50-col-matrix.ts --benchmark",
    "matrix:fix": "bun 50-col-matrix.ts --fix",
    "matrix:ci": "bun 50-col-matrix.ts --ci --threshold medium",
    "test": "RUN_TESTS=1 bun test --cwd test",
    "typecheck": "bun --bun tsc --noEmit"
  },
  "type": "module",
  "trustedDependencies": ["node-sass"],
  "dependencies": {
    "lodash": "^4.17.23",
    "zod": "^4.3.6"
  }
