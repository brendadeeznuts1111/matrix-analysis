{
  "name": "nolarose-mcp-config",
  "version": "1.0.1",
  "description": "MCP server configuration with Bun native support and RSS integration",
  "type": "module",
  "main": "./src/cli.ts",
  "module": "./src/cli.ts",
  "bin": {
    "matrix": "./src/cli.ts",
    "nolarose-mcp-config": "./src/cli.ts",
    "tier1380": "./cli/tier1380.ts",
    "matrix-agent": "~/.matrix/matrix-agent.ts"
  },
  "exports": {
    ".": "./src/cli.ts",
    "./lib/*": "./src/lib/*.ts",
    "./commands/*": "./src/commands/*.ts"
  },
  "scripts": {
    "typecheck": "tsc --noEmit",
    "mcp:inspector": "bun x @modelcontextprotocol/inspector",
