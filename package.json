    "bunx-demo": "bun run examples/bunx-demonstration.ts",
    "bun-update-demo": "bun run examples/bun-update-complete-demo.ts",
    "build:memory": "bun run examples/bun-build-memory-bundle.ts",
    "build:memory:simple": "bun run examples/bun-build-simple-memory.ts",
    "build:output": "bun run examples/bun-build-output-handling.ts",
    "build:cwd-root": "bun run examples/bun-build-cwd-root.ts",
    "mcp:matrix": "bun mcp-bun-docs/matrix-view.ts",
    "matrix:preview": "bun examples/matrix-preview.ts",
    "matrix-preview": "bun examples/matrix-preview.ts",
    "mcp:ci-validate": "bun mcp-bun-docs/scripts/ci-validate-entries.ts",
    "mcp:secure-test": "bun mcp-bun-docs/scripts/secure-test-runner.ts",
    "matrix:validate": "bun mcp-bun-docs/scripts/matrix-validate.ts",
    "matrix:rss": "bun mcp-bun-docs/scripts/matrix-rss.ts",
    "rss:verify": "bun mcp-bun-docs/scripts/verify-rss-canonicalization.ts",
    "rss:parse": "bun mcp-bun-docs/examples/rss-parse.example.ts",
    "docs:search": "bun tools/bun-docs.ts search",
    "docs:entry": "bun tools/bun-docs.ts entry",
    "docs:link": "bun tools/bun-docs.ts link",
    "docs:terms": "bun tools/bun-docs.ts terms",
    "docs:globals": "bun tools/bun-docs.ts globals",
    "docs:xrefs": "bun tools/bun-docs.ts xrefs",
    "docs:feedback": "bun tools/bun-docs.ts feedback",
    "docs:shop": "bun tools/bun-docs.ts shop",
    "docs:blog": "bun tools/bun-docs.ts blog",
    "docs:guides": "bun tools/bun-docs.ts guides",
    "docs:rss": "bun tools/bun-docs.ts rss",
    "docs:repo": "bun tools/bun-docs.ts repo",
    "docs:deps": "bun tools/bun-docs.ts deps",
    "docs:compat": "bun tools/bun-docs.ts compat",
    "docs:reference": "bun tools/bun-docs.ts reference",
    "docs:latest": "bun tools/bun-docs.ts latest",
    "docs:version": "bun tools/bun-docs.ts version",
    "analyze": "bun tools/analyze.ts",
    "analyze:scan": "bun tools/analyze.ts scan",
    "analyze:types": "bun tools/analyze.ts types",
    "analyze:props": "bun tools/analyze.ts props",
    "analyze:deps": "bun tools/analyze.ts deps",
    "analyze:complexity": "bun tools/analyze.ts complexity",
    "analyze:classes": "bun tools/analyze.ts classes",
    "analyze:strength": "bun tools/analyze.ts strength",
    "analyze:rename": "bun tools/analyze.ts rename",
    "analyze:polish": "bun tools/analyze.ts polish",
    "analyze:coverage": "bun tools/analyze.ts coverage",
    "analyze:bench": "bun tools/analyze.ts --bench",
    "bun-docs": "bun tools/bun-docs.ts",
    "docs": "bun tools/bun-docs.ts",
    "tier1380": "bun cli/tier1380.ts",
    "t1380": "bun cli/tier1380.ts",
    "tier1380:bench": "bun cli/tier1380.ts color generate --wcag=aa --formats=all",
    "tier1380:seal": "bun mcp-bun-docs/scripts/tier1380-seal.ts",
    "mcp:rss": "bun x rss-mcp",
    "team:add": "bun cli/tier1380.ts team add",
    "team:remove": "bun cli/tier1380.ts team remove",
    "team:promote": "bun cli/tier1380.ts team promote",
    "team:demote": "bun cli/tier1380.ts team demote",
    "team:hierarchy": "bun cli/tier1380.ts team hierarchy",
    "team:show": "bun cli/tier1380.ts team show",
    "terminal:pty": "bun cli/tier1380.ts terminal launch",
    "dev": "bun run src/cli.ts",
    "matrix": "bun run src/cli.ts",
    "test": "bun test .claude src",
    "test:claude": "bun test .claude",
    "test:src": "bun test src",
    "test:commands": "bun test ./src/__tests__/commands/",
    "test:grep": "bun test --test-name-pattern",
    "test:tz": "TZ=${TZ:-UTC} bun test",
    "test:coverage": "bun test --coverage",
    "test:preload": "bun test --preload ./test-setup.ts --preload ./global-mocks.ts",
    "test:env": "bun test --env-file=.env.test",
    "test:ci": "bun test --config=ci",
    "test:secure": "bun scripts/test-secure.ts",
    "test:audit": "bun scripts/test-audit.ts",
    "test:matrix": "bun scripts/test-matrix.ts",
    "test:inheritance": "bun test --inherit=install.registry,install.cafile --preload=./security-mocks.ts",
    "test:dry-run": "bun test --dry-run",
    "test:verbose": "bun test --verbose",
    "test:validate": "bun mcp-bun-docs/scripts/validate-options.ts",
    "grep:options": "rg 'BUN_TEST_CLI_OPTIONS|--test-name-pattern|--timeout|--bail|--preload|--root|--config|--env-file|--coverage' mcp-bun-docs/ 2>/dev/null || true",
    "grep:timeout": "rg 'timeout|--timeout' mcp-bun-docs/ 2>/dev/null || true",
    "index:options": "bun mcp-bun-docs/scripts/index-options.ts",
    "migrate": "bun tools/arm64/wrap-migrator.ts",
    "migrate:dry": "bun tools/arm64/wrap-migrator.ts --dry-run",
    "migrate:verbose": "bun tools/arm64/wrap-migrator.ts --verbose",
    "benchmark": "bun tools/arm64/benchmark-arm64.ts",
    "benchmark:output": "bun tools/arm64/benchmark-arm64.ts --output",
    "guardian": "bun tools/arm64/guardian.ts",
    "verify:arm64": "bun tools/arm64/verify-arm64.ts",
    "verify:arm64:output": "bun tools/arm64/verify-arm64.ts --output",
    "demo": "bun tools/arm64/arm64-demo.ts",
    "spawn:monitor": "bun tools/spawn-monitor.ts validate",
    "spawn:check": "bun tools/spawn-monitor.ts check",
    "spawn:bench": "bun tools/spawn-monitor.ts validate -n 500",
    "password:hash": "bun tools/password-hash.ts hash",
    "password:verify": "bun tools/password-hash.ts verify",
    "password:benchmark": "bun tools/password-hash.ts benchmark",
    "tension:status": "bun tools/tension-status.ts",
    "tension:debug": "bash scripts/tension-debug-master.sh",
    "tension:search": "bun tools/bun-search.ts",
    "terminal:init": "bun .claude/core/terminal/cli.ts init",
    "terminal:status": "bun .claude/core/terminal/cli.ts status",
    "terminal:bind": "bun .claude/core/terminal/cli.ts bind",
    "terminal:unbind": "bun .claude/core/terminal/cli.ts unbind",
    "terminal:switch": "bun .claude/core/terminal/cli.ts switch",
    "terminal:matrix": "bun .claude/core/terminal/cli.ts matrix",
    "terminal:broadcast": "bun .claude/core/terminal/cli.ts broadcast",
    "terminal:list": "bun .claude/core/terminal/cli.ts list",
    "tbind": "bun .claude/core/terminal/cli.ts bind",
    "tstatus": "bun .claude/core/terminal/cli.ts status",
    "tmatrix": "bun .claude/core/terminal/cli.ts matrix",
    "tbroadcast": "bun .claude/core/terminal/cli.ts broadcast",
    "terminal:setup": "bun .claude/core/terminal/setup-session.ts",
    "agent": "bun ~/.matrix/matrix-agent.ts",
    "agent:init": "bun ~/.matrix/matrix-agent.ts init",
    "agent:status": "bun ~/.matrix/matrix-agent.ts status",
    "agent:migrate": "bun ~/.matrix/matrix-agent.ts migrate",
    "agent:health": "bun ~/.matrix/scripts/health-check.ts",
    "agent:profile": "bun ~/.matrix/matrix-agent.ts profile",
    "agent:tier1380": "bun ~/.matrix/matrix-agent.ts tier1380",
    "globals:table": "bun tools/bun-globals-master.ts --format=md --output=docs/reference/bun-globals-reference.md",
    "globals:csv": "bun tools/bun-globals-master.ts --format=csv --output=docs/reference/bun-globals-reference.csv",
    "globals:json": "bun tools/bun-globals-master.ts --format=json --output=docs/reference/bun-globals-reference.json",
    "globals:interactive": "bun tools/bun-globals-master.ts --interactive",
    "omega:demo": "bun examples/omega-master-demo.ts",
    "omega:dashboard": "bun examples/omega-master-demo.ts --dashboard",
    "benchmark:spawn": "bun benchmarks/spawn-benchmark.ts",
    "benchmark:hash": "bun benchmarks/bun-hash-benchmark.ts",
    "benchmark:all": "bun benchmarks/spawn-benchmark.ts && bun benchmarks/bun-hash-benchmark.ts"
  },
  "dependencies": {
    "@modelcontextprotocol/server-filesystem": "latest",
    "@modelcontextprotocol/server-sequential-thinking": "latest",
    "@modelcontextprotocol/server-puppeteer": "latest",
    "@upstash/context7-mcp": "latest",
    "mcp-bun": "latest",
    "rss-mcp": "^1.0.1",
    "ts-morph": "^21.0.0",
    "semver": "^7.5.4"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/node": "latest",
    "@types/semver": "^7.5.6"
  },
  "mcp": {
    "servers": {
      "bun": {
        "command": "bun",
        "args": ["run", "node_modules/mcp-bun/dist/mcp-bun.js"],
        "env": {
          "DISABLE_NOTIFICATIONS": "true"
        },
        "description": "Bun runtime integration for JavaScript/TypeScript execution (Bun native)"
      },
      "bun-docs": {
        "command": "bun",
        "args": ["mcp-bun-docs/index.ts"],
        "description": "SearchBun - Search Bun docs (bun.com/docs/mcp). API refs, code examples, guides."
      },
      "context7": {
        "command": "bun",
        "args": ["run", "node_modules/@upstash/context7-mcp/dist/index.js"],
        "description": "Context7 documentation and code search"
      },
      "filesystem": {
        "command": "bun",
        "args": ["run", "node_modules/@modelcontextprotocol/server-filesystem/dist/index.js", "/Users/nolarose"],
        "description": "File system access for project operations"
      },
      "sequential-thinking": {
        "command": "bun",
        "args": ["run", "node_modules/@modelcontextprotocol/server-sequential-thinking/dist/index.js"],
        "description": "Structured problem-solving and reasoning tool"
      },
      "puppeteer": {
        "command": "bun",
        "args": ["run", "node_modules/@modelcontextprotocol/server-puppeteer/dist/index.js"],
        "description": "Browser automation for testing and web scraping",
        "env": {
          "HEADLESS": "true"
        }
      },
      "rss": {
        "command": "bun",
        "args": ["run", "node_modules/rss-mcp/dist/index.js"],
        "description": "RSS feed reader and parser for news aggregation",
        "env": {
          "RSS_MAX_ITEMS": "50",
          "RSS_CACHE_TTL": "300"
        }
      }
    }
  },
  "keywords": ["bun", "mcp", "dashboard", "telemetry"]
}
