{
  "name": "matrix-analysis",
  "version": "3.1.0",
  "author": "nolarose",
  "devDependencies": {
    "typescript": "^5.7.0"
  },
  "peerDependencies": {
    "@types/bun": "^1.3.6"
  },
  "description": "URLPattern Analysis Matrix with 247 columns - Performance optimized",
  "license": "MIT",
  "peerDependenciesMeta": {
    "@types/bun": {
      "optional": true
    }
  },
  "scripts": {
    "matrix": "bun 50-col-matrix.ts",
    "matrix:audit": "bun 50-col-matrix.ts --audit",
    "matrix:benchmark": "bun 50-col-matrix.ts --benchmark",
    "matrix:fix": "bun 50-col-matrix.ts --fix",
    "matrix:ci": "bun 50-col-matrix.ts --ci --threshold medium",
    "test": "RUN_TESTS=1 bun test --cwd test",
    "typecheck": "bun --bun tsc --noEmit"
  },
  "type": "module",
  "trustedDependencies": ["node-sass"],
  "dependencies": {
    "lodash": "^4.17.21",
    "zod": "npm:zod"
  }
}
