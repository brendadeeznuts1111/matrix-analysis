    "buffer:simd:comparison": "bun run benchmarks/buffer-simd-comparison.ts",
    "build:files:types": "bun run examples/bun-build-files-content-types.ts",
    "build:files:practical": "bun run examples/bun-build-files-practical.ts",
    "build:content-types": "bun run examples/bun-build-content-types.ts",
    "build:outdir": "bun run examples/bun-build-outdir.ts",
    "build:publicpath": "bun run examples/bun-build-publicpath.ts",
    "build:naming": "bun run examples/bun-build-naming.ts",
    "build:env": "bun run examples/bun-build-env.ts",
    "build:env-prefix": "bun run examples/bun-build-env-prefix.ts",
    "build:sourcemap-minify": "bun run examples/bun-build-sourcemap-minify.ts",
