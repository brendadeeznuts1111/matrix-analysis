    "mcp:bun-test:suite": "bun test ./examples/bun-mcp.test.ts ./mcp-bun-docs/search-bun.test.ts",
    "mcp:example:manifest": "bun mcp-bun-docs/examples/manifest-fetch.example.ts",
    "mcp:example:search": "bun mcp-bun-docs/examples/search-bun.example.ts",
    "mcp:example:spawn-sync": "bun mcp-bun-docs/examples/spawn-sync-bench.example.ts",
    "mcp:example:buffer-includes": "bun mcp-bun-docs/examples/buffer-includes-bench.example.ts",
