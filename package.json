  "mcp": {
    "servers": {
      "bun": {
        "command": "bun",
        "args": ["run", "node_modules/mcp-bun/dist/mcp-bun.js"],
        "env": {
          "DISABLE_NOTIFICATIONS": "true"
        },
        "description": "Bun runtime integration for JavaScript/TypeScript execution (Bun native)"
      },
      "bun-docs": {
        "command": "bun",
        "args": ["mcp-bun-docs/index.ts"],
        "description": "SearchBun - Search Bun docs (bun.com/docs/mcp). API refs, code examples, guides."
      },
      "context7": {
        "command": "bun",
        "args": ["run", "node_modules/@upstash/context7-mcp/dist/index.js"],
        "description": "Context7 documentation and code search"
      },
      "filesystem": {
        "command": "bun",
        "args": ["run", "node_modules/@modelcontextprotocol/server-filesystem/dist/index.js", "/Users/nolarose"],
        "description": "File system access for project operations"
      },
      "sequential-thinking": {
        "command": "bun",
        "args": ["run", "node_modules/@modelcontextprotocol/server-sequential-thinking/dist/index.js"],
        "description": "Structured problem-solving and reasoning tool"
      },
      "puppeteer": {
        "command": "bun",
        "args": ["run", "node_modules/@modelcontextprotocol/server-puppeteer/dist/index.js"],
