{
  "name": "bun-apis-slides",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "build": "bun src/build.ts",
    "build:light": "bun src/build.ts light",
    "watch": "bun src/build.ts --watch",
    "preview": "bun src/preview.ts",
    "serve": "bun src/server.ts",
    "open": "open -na 'Google Chrome' --args --profile-directory=BunApiSlides",
    "dev": "bun run build && bun run open file://$PWD/dist/index.html",
    "feedback": "bun src/feedback-server.ts",
    "feedback:stats": "curl -s http://localhost:3001/feedback/stats | bun -e 'console.log(Bun.inspect.table(await Bun.stdin.json()))'"
  },
  "dependencies": {},
  "devDependencies": {}
}
